<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias alias="Integer" type="java.lang.Integer"/>
        <typeAlias alias="Long" type="java.lang.Long"/>
        <typeAlias alias="BigDecimal" type="java.math.BigDecimal"/>
        <typeAlias alias="HashMap" type="java.util.HashMap"/>
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap"/>
        <typeAlias alias="ArrayList" type="java.util.ArrayList"/>
        <typeAlias alias="LinkedList" type="java.util.LinkedList"/>
    </typeAliases>
<!--    <plugins>-->
<!--        &lt;!&ndash;默认情况下会使用 PageHelper 方式进行分页，如果想要实现自己的分页逻辑，-->
<!--          可以实现 Dialect(com.github.pagehelper.Dialect) 接口，然后配置该属性为实现类的全限定名称。&ndash;&gt;-->
<!--&lt;!&ndash;        <plugin interceptor="com.github.pagehelper.PageInterceptor">&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;4.0.0以后版本可以不设置该参数&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="helperDialect" value="mysql"/>&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;默认值为 false，该参数对使用 RowBounds 作为分页参数时有效。&ndash;&gt;-->
<!--&lt;!&ndash;            当该参数设置为 true 时，会将 RowBounds 中的 offset 参数当成 pageNum 使用，可以用页码和页面大小两个参数进行分页。&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="offsetAsPageNum" value="false"/>&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;默认值为false，该参数对使用 RowBounds 作为分页参数时有效。&ndash;&gt;-->
<!--&lt;!&ndash;            当该参数设置为true时，使用 RowBounds 分页会进行 count 查询。&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="rowBoundsWithCount" value="false"/>&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;默认值为 false，当该参数设置为 true 时，如果 pageSize=0 或者 RowBounds.limit = 0&ndash;&gt;-->
<!--&lt;!&ndash;            就会查询出全部的结果（相当于没有执行分页查询，但是返回结果仍然是 Page 类型）。&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="pageSizeZero" value="true"/>&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;分页合理化参数，默认值为false。当该参数设置为 true 时，pageNum<=0 时会查询第一页， pageNum>pages（超过总数时），&ndash;&gt;-->
<!--&lt;!&ndash;            会查询最后一页。默认false 时，直接根据参数进行查询。&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="reasonable" value="false"/>&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;为了支持startPage(Object params)方法，增加了该参数来配置参数映射，用于从对象中根据属性名取值，&ndash;&gt;-->
<!--&lt;!&ndash;            可以配置 pageNum,pageSize,count,pageSizeZero,reasonable，不配置映射的用默认值，默认值为:&ndash;&gt;-->
<!--&lt;!&ndash;            pageNum=pageNum;pageSize=pageSize;count=countSql;reasonable=reasonable;pageSizeZero=pageSizeZero。&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="paems" value="false"/>&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;支持通过 Mapper 接口参数来传递分页参数，默认值false，分页插件会从查询方法的参数值中，&ndash;&gt;-->
<!--&lt;!&ndash;            自动根据上面 params 配置的字段中取值，查找到合适的值时就会自动分页。&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="supportMethodsArguments" value="false"/>&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;默认值为 false。设置为 true 时，允许在运行时根据多数据源自动识别对应方言的分页 （不支持自动选择sqlserver2012，&ndash;&gt;-->
<!--&lt;!&ndash;            只能使用sqlserver&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="autoRuntimeDialect" value="false"/>&ndash;&gt;-->
<!--&lt;!&ndash;            &lt;!&ndash;默认值为 true。当使用运行时动态数据源或没有设置 helperDialect 属性自动获取数据库类型时，会自动获取一个数据库连接，&ndash;&gt;-->
<!--&lt;!&ndash;            通过该属性来设置是否关闭获取的这个连接，默认true关闭，设置为 false 后，不会关闭获取的连接，&ndash;&gt;-->
<!--&lt;!&ndash;            这个参数的设置要根据自己选择的数据源来决定。&ndash;&gt;&ndash;&gt;-->
<!--&lt;!&ndash;            <property name="closeConn" value="true"/>&ndash;&gt;-->
<!--&lt;!&ndash;        </plugin>&ndash;&gt;-->
<!--    </plugins>-->
</configuration>